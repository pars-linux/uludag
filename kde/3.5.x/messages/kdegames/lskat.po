msgid "&Player"
msgstr "&Oyuncu"

#: lskat.cpp:195
msgid "&Computer"
msgstr "&Bilgisayar"

#: lskat.cpp:196
msgid "&Remote"
msgstr "&Uzaktan"

#: lskat.cpp:198
msgid "Player &2 Played By"
msgstr "Oyuncu &2 'yi oynayan:"

#: lskat.cpp:200 lskat.cpp:201
msgid "Changing who plays player 2..."
msgstr "Kimin oyuncu 2'yi oynayacağı değiştiriliyor..."

#: lskat.cpp:204
msgid "&Level"
msgstr "&Seviye"

#: lskat.cpp:206
msgid "Change level..."
msgstr "Seviyeyi değiştir..."

#: lskat.cpp:207
msgid "Change the strength of the computer player."
msgstr "Bilgisayar oyuncusunun seviyesini değiştir."

#: lskat.cpp:209
msgid "&Normal"
msgstr "&Normal"

#: lskat.cpp:210
msgid "&Advanced"
msgstr "&Gelişmiş"

#: lskat.cpp:211

<comarSecurityPolicy>
    <!-- comar call permissions on the model -->
    <model>
        <admin>
            <!-- members of the wheel group has admin access to all comar classes -->
            <group name="wheel"/>
            <!-- test rule: users group has admin access on Net.Link
            <class name="Net.Link">
                <group name="users"/>
            </class>
            -->
        </admin>
        
        <user>
            <!-- members of the users group has user access to all comar classes -->
            <group name="users"/>
        </user>
        
        <guest>
            <!-- no guess support yet -->
        </guest>
    </model>
</comarSecurityPolicy>

# Compressed Archives
application/x-pisi; pm-install '%s'; -n "$DISPLAY" ; nametemplate=%s.pisi
application/x-gzip; ark '%s'; -n "$DISPLAY" ; nametemplate=%s.gz
application/x-zip; ark '%s'; -n "$DISPLAY" ; nametemplate=%s.zip
application/x-tar; ark '%s'; -n "$DISPLAY" ; nametemplate=%s.tar
application/x-bzip2; ark '%s'; -n "$DISPLAY" ; nametemplate=%s.bz2


  # This template has no interface.
  #%]

[% PROCESS global/variables.none.tmpl %]

[% PROCESS global/header.html.tmpl
           title = "Oylarınızı silmek istiyor musunuz?"
 %]

<p>
  [% terms.Bug %] raporları için kulllandığınızt tüm oyları silmek üzeresiniz. Şu ana kadar
  kullandığınız tüm [% terms.bug %] raporu oylarınızı silmek istediğinize emin misiniz?
</p>

<form action="votes.cgi" method="post">
    <input type="hidden" name="action" value="vote">
  <p>
    <input type="radio" name="delete_all_votes" value="1">
    Evet, tüm oylarımı sil
  </p>
  <p>
    <input type="radio" name="delete_all_votes" value="0" checked="checked">
    Hayır, geri dön ve oylarımı tekrar göster
  </p>
  <p>
    <input type="submit" value="Gönder">
  </p>
</form>

[% PROCESS global/footer.html.tmpl %]

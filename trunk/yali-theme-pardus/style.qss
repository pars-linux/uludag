/* #Special Containers */

    #Yali {
        background-color: rgb(50, 50, 50);
        background-image: url(:/gui/pics/bg.png);
        padding: 0px;
        margin: 0px;
    }

    #frame{
        background-color: rgba(0,0,0,70);
    }

    #headContainer {
        border:1px;
        border-bottom:1px solid rgba(255,255,255,100);
        background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1,
            stop:0 rgba(132, 132, 132, 140), stop:0.501481 rgba(70, 70, 70, 140),
            stop:0.503057 rgba(62, 62, 62, 140), stop:1 rgba(16, 16, 16, 140));
    }

    #menuContainer{
        border-top: 1px solid white;
        background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1,
            stop:0 rgba(132, 132, 132, 140), stop:0.501481 rgba(70, 70, 70, 140),
            stop:0.503057 rgba(62, 62, 62, 140), stop:1 rgba(16, 16, 16, 140));
    }

    #helpContentFrame {
        background-color:rgba(0,0,0,180);
        border-bottom-left-radius:3px;
        border-bottom-right-radius:3px;
    }

    #scrollAreaWidgetContents {
        background-color: rgba(255, 255, 255, 0);
    }

    #mainStack {
        background-color: rgba(255, 255, 255, 0);
    }


/* #QScrollArea */

    #mainContent {
        background-color: rgba(255, 255, 255, 0);
    }


/* #Special Buttons */

    #newButton {
        color:white;
        border:1px solid  rgba(34, 49, 60, 100);
        border-radius:2px;
        padding:1px 15px 1px 5px;
        background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1,
            stop:0 rgba(132, 132, 132, 255), stop:0.501481 rgba(70, 70, 70, 255),
            stop:0.503057 rgba(62, 62, 62, 255), stop:1 rgba(16, 16, 16, 255));
    }

    #newButton[popupMode="1"] {
        padding-right: 20px;
    }

    #newButton:hover {
        border:1px solid  rgba(34, 49, 60, 100);
        border-radius:2px;
        background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0,
            stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 255),
            stop:0.521739 rgba(166, 35, 29, 255), stop:0.531585 rgba(184, 46, 40, 255),
            stop:0.983696 rgba(168, 78, 74, 255), stop:1 rgba(210, 110, 110, 255))
    }

    #newButton:pressed {
        border:1px solid  rgba(34, 49, 60, 100);
        border-radius:2px;
        background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0,
            stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 255),
            stop:0.521739 rgba(166, 35, 29, 255), stop:0.531585 rgba(184, 46, 40, 255),
            stop:0.983696 rgba(168, 78, 74, 255), stop:1 rgba(210, 110, 110, 255))
    }

    #toggleHelp{
        border:0px;
        background-color: rgba(0,0,0,0)
    }

    #toggleHelp::flat:hover {
        border:1px solid  rgba(34, 49, 60, 100);
        border-radius:2px;
        background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0,
            stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 255),
            stop:0.521739 rgba(166, 35, 29, 255), stop:0.531585 rgba(184, 46, 40, 255),
            stop:0.983696 rgba(168, 78, 74, 255), stop:1 rgba(210, 110, 110, 255))
    }

    #toggleHelp::flat {
        border:0px;
        background-color: rgba(0,0,0,0)
    }

    #toggleHelp::flat:pressed {
        border:1px solid  rgba(34, 49, 60, 100);
        border-radius:2px;
        padding: 3px 10px 3px 10px;
        background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0,
            stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 255),
            stop:0.521739 rgba(166, 35, 29, 255), stop:0.531585 rgba(184, 46, 40, 255),
            stop:0.983696 rgba(168, 78, 74, 255), stop:1 rgba(210, 110, 110, 255))
    }

    #toggleHelp::flat:unchecked {
        border:1px solid  rgba(34, 49, 60, 100);
        border-radius:2px;
        padding: 3px 10px 3px 10px;
        background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0,
        stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 255),
        stop:0.521739 rgba(166, 35, 29, 255), stop:0.531585 rgba(184, 46, 40, 255),
        stop:0.983696 rgba(168, 78, 74, 255), stop:1 rgba(210, 110, 110, 255))
    }


    #toggleHelp::flat:checked {
        border:1px solid  rgba(34, 49, 60, 100);
        border-radius:2px;
        padding: 3px 10px 3px 10px;
        background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0,
            stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 255),
            stop:0.521739 rgba(166, 35, 29, 255), stop:0.531585 rgba(184, 46, 40, 255),

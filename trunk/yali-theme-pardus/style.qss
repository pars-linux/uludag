/* #Calendar Widget */

    #calendarWidget QWidget {
        border: 0px;
        border-radius: 2px;
        color:white;
        font-size: 13px;
        background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.498, y2:1,
            stop:0 rgba(88, 88, 88, 255), stop:0.494105 rgba(71, 71, 71, 255),
            stop:1 rgba(85, 84, 84, 255));
    }

    #calendarWidget QFrame {
        background-color: #eeeeee;
        border: 0px;
        border-radius: 2px;
        font-size: 13px
    }

    #calendarWidget QComboBox {
        color:white
    }

    #calendarWidget QWidget:hover {
        color: #bbbbbb;
    }


    #calendarWidget QAbstractItemView {
        color: black;
    }

    #calendarWidget QComboBox:!editable, QComboBox::drop-down:editable { color: white;}

    #calendarWidget QComboBox:!editable:hover, QComboBox::drop-down:editable:hover {
        color:white;
        border-radius:2px;
        background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0,
            stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 255),
            stop:0.521739 rgba(166, 35, 29, 255), stop:0.531585 rgba(184, 46, 40, 255),
            stop:0.983696 rgba(168, 78, 74, 255), stop:1 rgba(210, 110, 110, 255))
    }


/* #QDateEdit */

    QDateEdit {
        border: 0px;
        border-radius: 3px;
        color: #333333;
        background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.516304,y2:0.653,
            stop:0 rgba(171, 171, 171, 255), stop:1 rgba(255, 255, 255, 255));
    }

    QDateEdit:hover, QComboBox::drop-down:editable:hover { }

    QDateEdit::drop-down {
        subcontrol-origin: padding;
        subcontrol-position: top right;
        width: 22px;
        border-top-right-radius: 4px;
        border-bottom-right-radius: 3px;
        background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1,
            stop:0 rgba(132, 132, 132, 255), stop:0.501481 rgba(70, 70, 70, 255),
            stop:0.503057 rgba(62, 62, 62, 255), stop:1 rgba(16, 16, 16, 255));
    }

    QDateEdit::drop-down:hover {
        background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0,
            stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 255),
            stop:0.521739 rgba(166, 35, 29, 255), stop:0.531585 rgba(184, 46, 40, 255),
            stop:0.983696 rgba(168, 78, 74, 255), stop:1 rgba(210, 110, 110, 255));
    }

    QDateEdit::drop-down:on {
        subcontrol-origin: padding;
        subcontrol-position: top right;
        border-top-right-radius: 4px;
        border-bottom-right-radius: 3px;
        font-size: 15px;
        background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0,
            stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 255),
            stop:0.521739 rgba(166, 35, 29, 255), stop:0.531585 rgba(184, 46, 40, 255),
            stop:0.983696 rgba(168, 78, 74, 255), stop:1 rgba(210, 110, 110, 255));
    }

    QDateEdit::down-arrow {
        image: url(:/images/arrow-down.png);
    }

    QDateEdit::down-arrow:on {
        top: 1px;
    }



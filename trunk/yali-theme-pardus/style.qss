/* #QlistView */

    QListView {
        show-decoration-selected: 1; /* make the selection span the entire width of the view */
        color: #444444;
        border-bottom-left-radius: 3px;
        border-bottom-right-radius: 3px;
        font-size: 12px;
        text-decoration: none;
        background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.516304,
            y2:0.653, stop:0 rgba(171, 171, 171, 255), stop:1 rgba(255, 255, 255, 220));
    }

    QListView::item:alternate {
        background: #EEEEEE;
    }

    QListView::item:selected {
    }

    QListView::item:selected:!active {
        background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0,
            stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 255),
            stop:0.521739 rgba(166, 35, 29, 255), stop:0.531585 rgba(184, 46, 40, 255),
            stop:0.983696 rgba(168, 78, 74, 255), stop:1 rgba(210, 110, 110, 255));
        border: 1px solid rgba(255, 255, 255, 50); border-radius: 3px; color:white
    }

    QListView::item:selected:active {
        background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0,
            stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 255),
            stop:0.521739 rgba(166, 35, 29, 255), stop:0.531585 rgba(184, 46, 40, 255),
            stop:0.983696 rgba(168, 78, 74, 255), stop:1 rgba(210, 110, 110, 255));
        border: 1px solid rgba(255, 255, 255, 50); border-radius: 3px; color:white
    }

    QListView::item:hover {
        background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0,
            stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 200),
            stop:0.521739 rgba(166, 35, 29, 200), stop:0.531585 rgba(184, 46, 40, 200),
            stop:0.983696 rgba(168, 78, 74, 200), stop:1 rgba(210, 110, 110, 200));
        border: 1px solid rgba(255, 255, 255, 50); border-radius: 3px; color:white
    }


/* #QTimeEdit */

    QTimeEdit {
        color: #333333;
        background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.516304,

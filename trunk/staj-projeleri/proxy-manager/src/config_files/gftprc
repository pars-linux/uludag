# Config file for gFTP. Copyright (C) 1998-2003 Brian Masney
# <masneyb@gftp.org>. Warning: Any comments that you add to this file WILL be
# overwritten. If a entry has a (*) in it's comment, you can't change it
# inside gFTP

# Dosyaları görüntülemek için kullanılacak öntanımlı dosya
# göstericisi. Eğer boş bırakılırsa dahili izleyici kullanılacaktır.
view_program=

# Dosyaları düzenlemek için kullanılan öntanımlı program.
edit_program=

# Başlangıçta gFTP'nin gireceği dizin
startup_directory=

# GTK+ portu için azami kayıt penceresi boyutu (bayt)
max_log_window_size=5000

# This is a comma separated list of charsets to try to convert the remote
# messages to the current locale
remote_charsets=

# This is the value of LC_TIME for the remote site. This is so that dates can
# be parsed properly in the directory listings.
remote_lc_time=

# The number of seconds to keep cache entries before they expire.
cache_ttl=3600

# Yeni dosya transferlerini daha öncekilerin üzerine ekle
append_transfers=1

# Bir seferde sadece bir dosya mı taşınsın?
one_transfer=1

# Dosyaların üzerine yaz veya iletime devam et
overwrite_default=0

# Preserve file permissions of transfered files
preserve_permissions=1

# Preserve file times of transfered files
preserve_time=1

# Her dosya transferinden sonra liste kutusunu yenile
refresh_files=0

# Önce dizinleri göster
sort_dirs_first=1

# Listedeki gizli dosyaları gösterir.
show_hidden_files=1

# Show the file transfer status in the titlebar
show_trans_in_title=0

# Automatically start the file transfers when they get queued
start_transfers=1

# Allow entering manual commands in the GUI (functions like the text port)
cmd_in_gui=0

# Ağ girdi/çıktısı için zaman aşımı. Bu programı kullanma ile ilgili
# zaman aşımı değildir.
network_timeout=120

# The number of auto-retries to do. Set this to 0 to retry indefinitely
retries=3

# Bağlantı tekrarları arasındaki bekleme süresi
sleep_time=30

# Bir dosya transferinin en yüksek hızı (kapatmak için 0 girin)
maxkbs=0.00

# The block size that is used when transfering files. This should be a
# multiple of 1024.
trans_blksize=20480

# Kullanılacak öntanımlı protokolü belirler
default_protocol=FTP

# Enable IPv6 support
enable_ipv6=1

# Dosya listesindeki bir dosya üzerine çift tıklanması halinde hangi
# işlemin gerçekleştileceğini belirler. 0=Dosyayı Aç 1=Dosyayı Düzenle
# 2=Dosyayı Al.
list_dblclk_action=2

# Yerel dosya liste kutusunun öntanımlı genişliği
listbox_local_width=320

# Uzaktaki dosya liste kutusunun öntanımlı genişliği
listbox_remote_width=321

# Yerel/uzak dosya liste kutusu öntanımlı boyutları
listbox_file_height=265

# Aktarma liste kutusunun öntanımlı yüksekliği
transfer_height=80

# Kayıt penceresinin öntanımlı yüksekliği
log_height=105

# Aktarma penceresinin dosya adı sütununun genişliği. 0 yapılırsa
# otomatik olarak boyutlandırılır.
file_trans_column=100

host_value=

port_value=

user_value=

# Sıralanacak öntanımlı sütun
local_sortcol=file

# Artan veya azalan şekilde sırala
local_sortasds=ascending

# Sıralanacak öntanımlı sütun
remote_sortcol=file

# Artan veya azalan şekilde sırala
remote_sortasds=ascending

# Dosya liste kutusundaki dosya adı sütununun genişliği. 0 yapılırsa
# otomatik olarak boyutlandırılır. -1 yapılırsa bu sütun gösterilmez.
local_file_width=100

# Dosya liste kutularındaki boyut sütunların genişliği. Bu değeri 0
# yaparak sütunların otomatik boyutlandırılmasını sağlayabilirsiniz.
# Eğer bu değer -1 ise bu sütun kapatılacaktır
local_size_width=85

# Dosya liste kutularındaki kullanıcı sütunların genişliği. Bu değeri
# 0 yaparak sütunların otomatik boyutlandırılmasını sağlayabilirsiniz.
# Eğer bu değer -1 ise bu sütun kapatılacaktır
local_user_width=75

# Dosya liste kutularındaki grup sütunların genişliği. Bu değeri 0
# yaparak sütunların otomatik boyutlandırılmasını sağlayabilirsiniz.
# Eğer bu değer -1 ise bu sütun kapatılacaktır
local_group_width=76

# Dosya liste kutularındaki veri sütunların genişliği. Bu değeri 0
# yaparak sütunların otomatik boyutlandırılmasını sağlayabilirsiniz.
# Eğer bu değer -1 ise bu sütun kapatılacaktır
local_date_width=120

# Dosya liste kutularındaki özellik sütunların genişliği. Bu değeri 0
# yaparak sütunların otomatik boyutlandırılmasını sağlayabilirsiniz.
# Eğer bu değer -1 ise bu sütun kapatılacaktır
local_attribs_width=75

# Dosya liste kutusundaki dosya adı sütununun genişliği. 0 yapılırsa
# otomatik olarak boyutlandırılır. -1 yapılırsa bu sütun gösterilmez.
remote_file_width=100

# Dosya liste kutularındaki boyut sütunların genişliği. Bu değeri 0
# yaparak sütunların otomatik boyutlandırılmasını sağlayabilirsiniz.
# Eğer bu değer -1 ise bu sütun kapatılacaktır
remote_size_width=85

# Dosya liste kutularındaki kullanıcı sütunların genişliği. Bu değeri
# 0 yaparak sütunların otomatik boyutlandırılmasını sağlayabilirsiniz.
# Eğer bu değer -1 ise bu sütun kapatılacaktır
remote_user_width=75

# Dosya liste kutularındaki grup sütunların genişliği. Bu değeri 0
# yaparak sütunların otomatik boyutlandırılmasını sağlayabilirsiniz.
# Eğer bu değer -1 ise bu sütun kapatılacaktır
remote_group_width=76

# Dosya liste kutularındaki veri sütunların genişliği. Bu değeri 0
# yaparak sütunların otomatik boyutlandırılmasını sağlayabilirsiniz.
# Eğer bu değer -1 ise bu sütun kapatılacaktır
remote_date_width=120

# Dosya liste kutularındaki özellik sütunların genişliği. Bu değeri 0
# yaparak sütunların otomatik boyutlandırılmasını sağlayabilirsiniz.
# Eğer bu değer -1 ise bu sütun kapatılacaktır
remote_attribs_width=75

# Sunucuya gönderilecek komutların renkleri
send_color=0:8600:0

# Sunucudan alınacak komutların renkleri
recv_color=0:0:ffff

# Hata mesajlarının renkleri
error_color=ffff:0:0

# Kalan kayıt mesajlarının renkleri
misc_color=a000:8d00:4600

# This is the password that will be used whenever you log into a remote FTP
# server as anonymous
email=

# Güvenlik duvarı
ftp_proxy_host=212.21.212.121

# Güvenlik duvarında bağlanılacak port
ftp_proxy_port=3223

# Güvenlik duvarı kullanıcı adı
ftp_proxy_username=

# Güvenlik duvarı parolası
ftp_proxy_password=

# Güvenlik duvarı hesabı (seçimlik)
ftp_proxy_account=

# This specifies how your proxy server expects us to log in. You can specify a
# 2 character replacement string prefixed by a % that will be replaced with
# the proper data. The first character can be either p for proxy or h for the
# host of the FTP server. The second character can be u (user), p (pass), h
# (host), o (port) or a (account). For example, to specify the proxy user, you
# can you type in %pu
proxy_config=

# If this is enabled, then the remote FTP server's PASV IP address field will
# be ignored and the host's IP address will be used instead. This is often
# needed for routers giving their internal rather then their external IP
# address in a PASV reply.
ignore_pasv_address=0

# If this is enabled, then the remote FTP server will open up a port for the
# data connection. If you are behind a firewall, you will need to enable this.
# Generally, it is a good idea to keep this enabled unless you are connecting
# to an older FTP server that doesn't support this. If this is disabled, then
# gFTP will open up a port on the client side and the remote server will
# attempt to connect to it.
passive_transfer=1

# The remote FTP server will attempt to resolve symlinks in the directory
# listings. Generally, this is a good idea to leave enabled. The only time you
# will want to disable this is if the remote FTP server doesn't support the -L
# option to LIST
resolve_symlinks=1

# If you are transfering a text file from Windows to UNIX box or vice versa,
# then you should enable this. Each system represents newlines differently for
# text files. If you are transfering from UNIX to UNIX, then it is safe to
# leave this off. If you are downloading binary data, you will want to disable
# this.
ascii_transfers=0

# SSL entropy file
entropy_source=/dev/urandom

# The maximum number of bytes to seed the SSL engine with
entropy_len=1024

# Verify SSL Peer
verify_ssl_peer=1

# Güvenlik duvarı
http_proxy_host=212.21.212.121

# Güvenlik duvarında bağlanılacak port
http_proxy_port=3223

# Güvenlik duvarı kullanıcı adı
http_proxy_username=

# Güvenlik duvarı parolası
http_proxy_password=

# HTTP/1.1 ya da HTTP/1.0 kullanmak istiyor musunuz?
use_http11=1

# SSH programının yolu
ssh_prog_name=

# SSH programına aktarılacak ek parametreler
ssh_extra_params=

# SSH bağlantıları için kullanıcı adı/parola iste
ssh_need_userpass=1

# Bu kısım hangi makinaların alt ağda olduğunu belirtir ve eğer uygunsa
# vekil sunucuya gidilmez. Kullanım: dont_use_proxy=.alanadı veya
# dont_use_proxy=ağ numarası/maskesi

# ext=dosya uzantısı:XPM dosyası:Ascii veya ikilik (A veya B):gösterici
# program. Not: Dosya uzantısı dışındaki argümanların kullanımı
# isteğe bağlıdır
ext=.pdf::B:xpdf
ext=.rpm:rpm.xpm:B:
ext=.deb:deb.xpm:B:
ext=.diff:diff.xpm: :
ext=.htm:world.xpm:A:
ext=.html:world.xpm:A:
ext=.xcf:img.xpm:B:
ext=.psd:img.xpm:B:
ext=.xpm:img.xpm:B:
ext=.bmp:img.xpm:B:
ext=.tif:img.xpm:B:
ext=.tiff:img.xpm:B:
ext=.png:img.xpm:B:
ext=.jpg:img.xpm:B:
ext=.mp3:sound.xpm:B:
ext=.mid:sound.xpm:B:
ext=.wav:sound.xpm:B:
ext=.bz2:tar.xpm:B:
ext=.gz:tar.xpm:B:
ext=.1:man.xpm:B:xman
ext=.2:man.xpm:B:xman
ext=.3:man.xpm:B:xman
ext=.4:man.xpm:B:xman
ext=.5:man.xpm:B:xman
ext=.6:man.xpm:B:xman
ext=.7:man.xpm:B:xman
ext=.8:man.xpm:B:xman
ext=.tar:tar.xpm:B:
ext=.tgz:tar.xpm:B:

msgstr "Bewerk LVM-volumegroep: %s"

#: yali/gui/lvm_gui.py:67
msgid "Edit LVM Volume Group"
msgstr "LVM-volumegroep bewerken"

#: yali/gui/lvm_gui.py:93
msgid "Invalid Volume Group Name"
msgstr "Ongeldige naam volumegroep"

#: yali/gui/lvm_gui.py:101
msgid "Name in use"
msgstr "Naam is in gebruik"

#: yali/gui/lvm_gui.py:102
#, python-format

#menuContainer{
	background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1, stop:0 rgba(132, 132, 132, 255), stop:0.501481 rgba(70, 70, 70, 255), stop:0.503057 rgba(62, 62, 62, 255), stop:1 rgba(16, 16, 16, 255));
	border-top: 1px solid white
}

QSlider::groove:horizontal {
    height: 12px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #B1B1B1, stop:1 #FFFFFF);
    margin: 2px 0;
}

QSlider::handle:horizontal {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #5A2848, stop:1 #8f8f8f);
    border: 1px solid #5c5c5c;
    width: 18px;
    margin: 0 0;
    border-radius: 2px;
}

 QToolButton {
	border:1px solid  rgba(34, 49, 60, 100);
	border-radius:2px;
	background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1, stop:0 rgba(132, 132, 132, 255), stop:0.501481 rgba(70, 70, 70, 255), stop:0.503057 rgba(62, 62, 62, 255), stop:1 rgba(16, 16, 16, 255));
	padding:1px 0px 1px 5px;
 }

 QToolButton[popupMode="1"] {
	padding-right: 20px;
 }

 QToolButton:hover{
	border:1px solid  rgba(34, 49, 60, 100);
	border-radius:2px;
	background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 255), stop:0.521739 rgba(166, 35, 29, 255), stop:0.531585 rgba(184, 46, 40, 255), stop:0.983696 rgba(168, 78, 74, 255), stop:1 rgba(210, 110, 110, 255))
}

 QToolButton:pressed {
	border:1px solid  rgba(34, 49, 60, 100);
	border-radius:2px;
	/*padding: 3px 10px 3px 10px;*/
	background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 255), stop:0.521739 rgba(166, 35, 29, 255), stop:0.531585 rgba(184, 46, 40, 255), stop:0.983696 rgba(168, 78, 74, 255), stop:1 rgba(210, 110, 110, 255))

 }

 /* the subcontrols below are used only in the MenuButtonPopup mode */
 QToolButton::menu-button {
	border-left:1px solid  rgba(34, 49, 60, 100);
     border-bottom-right-radius: 6px;
     /* 16px width + 4px for border = 20px allocated above */
     width: 16px;
 }

QToolButton::menu-arrow {
     image: url(:/images/treeview-branch-open.png);
 }

 QToolButton::menu-arrow:open {
     top: 1px; left: 1px; /* shift it a bit */
 }

 /* the subcontrols below are used only in the MenuButtonPopup mode */
 QToolButton::menu-button {
     border-top-right-radius: 3px;
     border-bottom-right-radius: 3px;
     width: 16px;
 }






QListView {
     show-decoration-selected: 1; /* make the selection span the entire width of the view */
 }


    QComboBox::drop-down:on {
        subcontrol-origin: padding;
        subcontrol-position: top right;
        width: 22px;
        border-top-right-radius: 4px;
        border-bottom-right-radius: 3px;
    }

    QComboBox::down-arrow {
        image: url(/usr/share/icons/crystalsvg/16x16/actions/1downarrow.png);
    }

    QComboBox::down-arrow:on { /* shift the arrow when popup is open */
        top: 1px;
        left: 1px;
    }

    QComboBox::down-arrow {
        right: 0px;
        image: url(:/images/arrow-down.png);
    }

    QComboBox::down-arrow:on {
        top: 1px;
    }

    QComboBox QAbstractItemView {
        color: #222222;
        selection-background-color: gray;
        border: 0px;
    }


/* #QLineEdit */

    QLineEdit {
        background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.516304, y2:0.653,
            stop:0 rgba(171, 171, 171, 255), stop:1 rgba(255, 255, 255, 255));
        color: #333333;
        border: 1px solid gray;
        border-radius: 3px;
        font-size: 17px
    }

    QLineEdit:hover {
        background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0.9,
            stop:0 rgba(171, 3, 3, 255), stop:1 rgba(255, 255, 255, 255));
        color: #333333;
        border: 1px solid gray;
        border-radius: 3px;
    }

    QLineEdit:disabled {
        border: 1px solid gray;
        border-radius: 2px
    }


/* #QTextEdit */

    QTextEdit {
        border: 1px solid #6b6b6b;
        border-radius: 4px;
        alternate-background-color: #222222;
        selection-background-color: gray;
    }


/* #QTreeView */

    QTreeView {
        border: 0px solid rgba(255,255,255,100);
        border-radius: 3px;
        selection-background-color: gray;
    }

    QTreeView::branch:has-children:!has-siblings:closed,
    QTreeView::branch:closed:has-children:has-siblings {
        border-image: none;
        image: url(:/gui/pics/expand.png);;
    }

    QTreeView::branch:open:has-children:!has-siblings,
    QTreeView::branch:open:has-children:has-siblings  {
        border-image: none;
        image: url(:/gui/pics/collapse.png);;
    }


/* #QHeaderView Table Column Headers */

    QHeaderView::section {
        background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                          stop:0 #616161, stop: 0.5 #505050,
                          stop: 0.6 #434343, stop:1 #656565);
        font-size: 8pt;
        color: white;
        padding-left:5px;
        border-right: 1px solid rgba(255,255,255,20);
        border-left: 1px solid rgba(255,255,255,20);
        border-top: 1px solid rgba(255,255,255,20);
        border-bottom: 1px solid rgba(255,255,255,20);
        height: 27px
    }

    /* style the sort indicator */
    QHeaderView::down-arrow {
        image: url(:/images/arrow-down.png);
    }

    QHeaderView::up-arrow {
        image: url(:/images/arrow-up.png);
    }


/* #QProgressBar */

    QProgressBar {
        border-radius: 2px;
        text-align: center;
        color: #222222;
        background-color: white;
    }

    QProgressBar:chunk {
        background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0,
            stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 255),
            stop:0.521739 rgba(166, 35, 29, 255), stop:0.531585 rgba(184, 46, 40, 255),
            stop:0.983696 rgba(168, 78, 74, 255), stop:1 rgba(210, 110, 110, 255));
        border: 1px solid rgba(255, 255, 255, 50);
        border-radius: 2px;
    }


/* #QScrollArea */

    QScrollArea::corner {
        background-color: rgba(255, 255, 255, 0);
        border: 0px;
    }

    QScrollBar:vertical {
        width: 13px;
        margin: 3px 3px 3px 3px;
        background-color: rgba(255, 255, 255, 0);
    }

    QScrollBar::handle:vertical {
        min-height: 16px;
        background-color: rgba(0,0,0, 150);
        border: 1px solid rgb(150, 150, 150);
        border-radius: 3px;
        padding:3px;
    }

    QScrollBar::handle:vertical:hover {
        min-height: 16px;
        background-color: rgba(50,50,50, 150);
        border-radius: 3px;
    }

    QScrollBar::add-line:vertical {
        border: 0px;
        height: 0px;
        subcontrol-position: bottom;
        subcontrol-origin: margin;
        background: transparent;
    }

    QScrollBar::sub-line:vertical {
        border: 0px;
        height: 0px;
        subcontrol-position: top;
        subcontrol-origin: margin;
        background: transparent;
    }

    QScrollBar:horizontal {
        height: 13px;
        margin: 3px 3px 3px 3px;
        background: transparent;
    }

    QScrollBar::handle:horizontal {
        min-width: 16px;
        background-color: rgba(0,0,0, 150);
        border: 1px solid rgb(150, 150, 150);
        border-radius: 3px;
    }

    QScrollBar::handle:horizontal:hover {
        min-width: 16px;
        background-color: rgba(50,50,50, 150);
        border-radius: 3px;
    }

    QScrollBar::add-line:horizontal {
        border: none;
        background: transparent;
        width: 0px;
        subcontrol-position: right;
        subcontrol-origin: margin;
    }

    QScrollBar::sub-line:horizontal {
        border: none;
        width: 0px;
        subcontrol-position: left;
        subcontrol-origin: margin;

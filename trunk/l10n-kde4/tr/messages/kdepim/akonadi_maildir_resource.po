#, kde-format
msgid "Maildir '%1' for collection '%2' is invalid."
msgstr "'%2' koleksiyonu için '%1' Maildir dizini geçersiz."

#: maildirresource.cpp:336
#, kde-format
msgid "Unable to rename maildir folder '%1'."
msgstr "'%1' maildir dizini oluşturulamadı."


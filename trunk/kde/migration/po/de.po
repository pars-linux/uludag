#: .tmp/ScrOptions.py:23
msgid "Selecting Options"
msgstr "Wähle die Funktion"

#: .tmp/ScrOptions.py:24 .tmp/ScrProgress.py:206 .tmp/ScrSummary.py:26
#: .tmp/ScrUserFiles.py:143 .tmp/ScrWelcome.py:23
msgid "Welcome to Migration Tool Wizard :)"
msgstr "Willkommen zum Migrations Assistenten"

#: .tmp/ScrOptions.py:61
msgid "Keep current wallpaper"
msgstr "Behalte das aktuelle Wallpaper"

#: .tmp/ScrOptions.py:63
msgid "Don't use wallpaper"
msgstr "Keinen Bildschirmhintergrund benutzen"

#: .tmp/ScrOptions.py:64
msgid "Does not change your wallpaper."
msgstr "Keine Veränderung am Bildschirmhintergrund."

#: .tmp/ScrOptions.py:84
msgid "Use my old wallpaper"
msgstr "Setze meinen alten Bildschirmhintergrund"

#: .tmp/ScrOptions.py:85
msgid "Copies your old wallpaper to Pardus and sets it as new background image."
msgstr "Kopiere Ihren alten Wallpaper zu Pardus und benutzt ihn als neuer Bildschirmhintergrund."

#: .tmp/ScrOptions.py:93 .tmp/ScrProgress.py:296
msgid "Bookmarks"
msgstr "Lesezeichen"

#: .tmp/ScrOptions.py:105
msgid "Firefox Bookmarks"
msgstr "Firefox Lesezeichen"

#: .tmp/ScrOptions.py:107
msgid "Copies your old Firefox bookmarks to Firefox under Pardus."
msgstr "Kopiere alte Firefox Lesezeichen zum neuen Firefox von Pardus."

#: .tmp/ScrOptions.py:116
msgid "Opera Bookmarks"
msgstr "Opera Lesezeichen"

#: .tmp/ScrOptions.py:118
msgid "Copies your old Opera bookmarks to Firefox under Pardus."
msgstr "Kopiere alte Opera Lesezeichen zum Firefox von Pardus."

#: .tmp/ScrOptions.py:127
msgid "Internet Explorer favorites"
msgstr "Internet Explorer Lesezeichen"

#: .tmp/ScrOptions.py:129
msgid "Copies your old Internet Explorer favorites to Firefox under Pardus."
msgstr "Kopiere alte Internet Explorer Lesezeichen zum Firefox von Pardus."

#: .tmp/ScrOptions.py:138
msgid "E-Mail and News Accounts"
msgstr "E-Mail- und Newskonten"

#: .tmp/ScrOptions.py:149
msgid "Windows Mail accounts"
msgstr "Windows E-mailkonten"

#: .tmp/ScrOptions.py:151 .tmp/ScrOptions.py:162
msgid "Copies your old mail and newsgroup accounts to KMail and KNode applications."
msgstr "Kopiere alte E-mail- und Newsgroupkonten zu KMail und KNode."

#: .tmp/ScrOptions.py:160
msgid "Thunderbird accounts"
msgstr "Thunderbirdkonten"

#: .tmp/ScrOptions.py:168
msgid "Copy e-mail messages from e-mail accounts"
msgstr "Kopiere E-mailnachrichten von den E-mailkonten"

#: .tmp/ScrOptions.py:170
msgid "Copies your e-mail messages to KMail from selected applications above."
msgstr "Kopiere Emailnachrichten von den obig ausgewählten Programmen zu KMail."

#: .tmp/ScrOptions.py:179 .tmp/ScrProgress.py:377
msgid "Instant Messenger Accounts"
msgstr "Instant Messenger Konten"

#: .tmp/ScrOptions.py:190

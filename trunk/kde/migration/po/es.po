#: .tmp/ScrOptions.py:23
msgid "Selecting Options"
msgstr "Seleccionando Opciones"

#: .tmp/ScrOptions.py:24 .tmp/ScrProgress.py:206 .tmp/ScrSummary.py:26
#: .tmp/ScrUserFiles.py:143 .tmp/ScrWelcome.py:23
msgid "Welcome to Migration Tool Wizard :)"
msgstr "Bienvenido al Asistente de Migración :)"

#: .tmp/ScrOptions.py:61
msgid "Keep current wallpaper"
msgstr "Mantener imagen de fondo actual"

#: .tmp/ScrOptions.py:63
msgid "Don't use wallpaper"
msgstr "No usar imagen de fondo"

#: .tmp/ScrOptions.py:64
msgid "Does not change your wallpaper."
msgstr "No cambiar fondo de escritorio."

#: .tmp/ScrOptions.py:84
msgid "Use my old wallpaper"
msgstr "Usar mi fondo antiguo"

#: .tmp/ScrOptions.py:85
msgid ""
"Copies your old wallpaper to Pardus and sets it as new background image."
msgstr "Copia su fondo antiguo a Pardus y lo establece como fondo nuevo."

#: .tmp/ScrOptions.py:93 .tmp/ScrProgress.py:296
msgid "Bookmarks"
msgstr "Marcadores"

#: .tmp/ScrOptions.py:105
msgid "Firefox Bookmarks"
msgstr "Marcadores de Firefox"

#: .tmp/ScrOptions.py:107
msgid "Copies your old Firefox bookmarks to Firefox under Pardus."
msgstr "Copia sus marcadores del Firefox antiguo al Firefox en Pardus."

#: .tmp/ScrOptions.py:116
msgid "Opera Bookmarks"
msgstr "Marcadores de Opera"

#: .tmp/ScrOptions.py:118
msgid "Copies your old Opera bookmarks to Firefox under Pardus."
msgstr "Copia sus marcadores del Opera antiguo al Firefox en Pardus."

#: .tmp/ScrOptions.py:127
msgid "Internet Explorer favorites"
msgstr "Marcadores de Internet Explorer"

#: .tmp/ScrOptions.py:129
msgid "Copies your old Internet Explorer favorites to Firefox under Pardus."
msgstr ""
"Copia sus marcadores del Internet Explorer antiguo al Firefox en Pardus."

#: .tmp/ScrOptions.py:138
msgid "E-Mail and News Accounts"
msgstr "Cuentas de E-Mail y Noticias"

#: .tmp/ScrOptions.py:149
msgid "Windows Mail accounts"
msgstr "Cuentas de Email de Windows"

#: .tmp/ScrOptions.py:151 .tmp/ScrOptions.py:162
msgid ""
"Copies your old mail and newsgroup accounts to KMail and KNode applications."
msgstr "Copia sus cuentas antiguas de email y noticias a KMail y KNode."

#: .tmp/ScrOptions.py:160
msgid "Thunderbird accounts"
msgstr "Cuentas de Thunderbird"

#: .tmp/ScrOptions.py:168
msgid "Copy e-mail messages from e-mail accounts"
msgstr "Copiar mensajes email de las cuentas de email"

#: .tmp/ScrOptions.py:170
msgid "Copies your e-mail messages to KMail from selected applications above."
msgstr ""
"Copia sus mensajes de email de los aplicativos seleccionados arriba, a KMail."

#: .tmp/ScrOptions.py:179 .tmp/ScrProgress.py:377
msgid "Instant Messenger Accounts"
msgstr "Cuentas de Mensajería Instantánea"

#: .tmp/ScrOptions.py:190
msgid "MSN accounts"
msgstr "Cuentas MSN"

#: .tmp/ScrOptions.py:192
msgid "Copies your MSN Messenger accounts to Kopete."
msgstr "Copia sus cuentas de MSN a Kopete."

#: .tmp/ScrOptions.py:201
msgid "GTalk accounts"
msgstr "Cuentas GTalk"

#: .tmp/ScrOptions.py:203
msgid "Copies your GTalk accounts to Kopete."
msgstr "Copia sus cuentas de GTalk a Kopete."

#: .tmp/ScrProgress.py:38
msgid "Please wait while applying changes..."
msgstr "Por favor espere mientras se están aplicando los cambios..."

#: .tmp/ScrProgress.py:48
msgid "Prepare: "
msgstr "Preparando:"

#: .tmp/ScrProgress.py:55
msgid "Apply: "
msgstr "Aplicando:"

#: .tmp/ScrProgress.py:112
msgid "Warning!"
msgstr "Alerta!"

#: .tmp/ScrProgress.py:205
msgid "Applying Changes"
msgstr "Aplicando cambios"

#: .tmp/ScrProgress.py:244
msgid "Wallpaper"
msgstr "Fondo de Escritorio"

#: .tmp/ScrProgress.py:254
msgid "Firefox bookmarks could not be loaded."
msgstr "No posible importar los marcadores de Firefox."

#: .tmp/ScrProgress.py:256
msgid "Firefox bookmarks loaded."
msgstr "Marcadores de Firefox importados."

#: .tmp/ScrProgress.py:266
msgid "Opera bookmarks could not be loaded."
msgstr "No posible importar los marcadores de Opera."

#: .tmp/ScrProgress.py:268
msgid "Opera bookmarks loaded."
msgstr "Marcadores de Opera importados."

#: .tmp/ScrProgress.py:278
msgid "Internet Explorer favorites could not be loaded."
msgstr "No posible importar los favoritos de Internet Explorer."

#: .tmp/ScrProgress.py:280
msgid "Internet Explorer favorites loaded."
msgstr "Favoritos de Internet Explorer importados."

#: .tmp/ScrProgress.py:293
msgid "Firefox is open. Please close it first to continue..."
msgstr "Firefox se está ejecutando. Favor cierrelo antes de continuar..."

#: .tmp/ScrProgress.py:305
msgid "Windows Mail accounts could not be loaded."
msgstr "No posible importar las cuentas de email de Windows."

#: .tmp/ScrProgress.py:307
msgid "Windows Mail accounts loaded."
msgstr "Cuentas email de Windows importados."

#: .tmp/ScrProgress.py:318
msgid "Thunderbird accounts could be loaded."
msgstr "No posible importar cuentas de email Thunderbird."

#: .tmp/ScrProgress.py:320
msgid "Thunderbird accounts loaded."
msgstr "Cuentas de Thunderbird importados."

#: .tmp/ScrProgress.py:330
msgid "MSN accounts could be loaded."
msgstr "No posible importar las cuentas del MSN."

#: .tmp/ScrProgress.py:332
msgid "MSN accounts loaded."
msgstr "Cuentas de MSN importados."

#: .tmp/ScrProgress.py:342
msgid "GTalk accounts could not be loaded."
msgstr "No posible importar las cuentas de GTalk."

#: .tmp/ScrProgress.py:344
msgid "GTalk accounts loaded."
msgstr "Cuentas de GTalk importados."

#: .tmp/ScrProgress.py:353
msgid "E-Mail Accounts"
msgstr "Cuentas Email"

#: .tmp/ScrProgress.py:361
msgid "E-Mail Messages"
msgstr "Mensajes Email"

#: .tmp/ScrProgress.py:369
msgid "News Accounts"
msgstr "Cuentas de Noticias"

# favor revisar, creo que se trata de las asociaciones de archivos de konqueror
#: .tmp/ScrProgress.py:389
msgid "Desktop Links"
msgstr "Enlaces de Escritorio"

#: .tmp/ScrProgress.py:396, python-format
msgid "Folder '%s' could not be created, please choose another folder!"
msgstr "No posible crear directorio '%s', por favor elija otro directorio!"

#: .tmp/ScrProgress.py:400
#, python-format
msgid ""
"You don't have permission to write to folder '%s', please choose another "
"folder!"
msgstr ""
"No tiene permisos de escritura en el directorio '%s', por favor elija otro "
"directorio!"

#: .tmp/ScrProgress.py:411, python-format
msgid ""
"Total size of files you've selected is %(size)d MB, but you have only %(free)"
"d MB of free space!"
msgstr ""
"El tamaño total de los archivos seleccionados es %(size)d MB, pero tiene "
"solamente %"
"(free)d MB espacio libre!"

#: .tmp/ScrProgress.py:425
msgid "Wallpaper changed."
msgstr "Imagen de fondo cambiado."

#: .tmp/ScrProgress.py:438
msgid "Bookmarks saved."
msgstr "Marcadores guardados."

#: .tmp/ScrProgress.py:450
msgid "Mail Accounts saved."
msgstr "Cuentas de Email guardados."

#: .tmp/ScrProgress.py:462
msgid "Accounts saved."
msgstr "Cuentas guardadas."

#: .tmp/ScrProgress.py:473
msgid "News Accounts saved."
msgstr "Cuentas de noticias guardados."

#: .tmp/ScrProgress.py:484
msgid "Instant Messenger Accounts saved."
msgstr "Cuentas de mensajería instantánea guardados."

#: .tmp/ScrProgress.py:493
#, python-format
msgid "Link '%s' created."
msgstr "Enlace '%s' creado."

#: .tmp/ScrProgress.py:506
msgid "Nothing done, because no option selected. You can close the wizard..."
msgstr ""
"Nada que hacer, porque no eligió ninguna opción. Puede cerrar el asistente..."

#: .tmp/ScrProgress.py:508
msgid "All operations completed. You can close the wizard..."
msgstr "Todas las operaciones completadas. Puede cerrar el asistente..."

#: .tmp/ScrSummary.py:25 .tmp/ui_summaryWidget.py:54
msgid "Summary"
msgstr "Resumen"


[%
   title = "$terms.Bug Taşıma"
   desc = "Başka bir kurulumla bu kurulum arasında $terms.bugs taşıma parametrelerini belirle"
%]

[% param_descs = {
  "move-enabled" => "Açıksa, $terms.Bugzilla belirli kişilerin belirtilen veritabanına $terms.bug taşımasına izin verir",

  "move-button-text" => "Taşı düğmesinin üzerine yazılan metin. $terms.BugGen nereye taşındığını açıklayın.",

  "move-to-url" => "$terms.BugsGen taşındığı veritabanının adresi.",

  "move-to-address" => "$terms.BugsAcc taşımak için, hedef veritabanına bir eposta gönderilir. Bu alan, veritabanının gelen $terms.bugs için dinlediği eposta adresidir.",

  "moved-from-address" => "$terms.BugsAcc taşımak için, hedef veritabanına bir eposta gönderilir. Bu alan, bu epostaları ve hata mesajlarını gönderen eposta adresidir.",

  movers => "$terms.BugsAcc taşıma ve (taşıma işleminin başarısız olması durumunda) taşınan epostaları yeniden açma izni olan kişilerin listesi.",

  "moved-default-product" => "Başka veritabanlarından buraya taşınan $terms.bugs bu ürüne atanır.",

  "moved-default-component" => "Başka veritabanlarından buraya taşınan $terms.bugs bu bileşene atanır." }
%]

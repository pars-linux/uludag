[%# Filter off the name here to be used multiple times below %]
[% name = name FILTER html %]

[% PROCESS global/header.html.tmpl 
  title = "'$name' isimli Eklenti Durumunu sil" 
%]

<p>
   [% attachcount %] eklentinin durumu '[% name %]' 'dur. Bunu silerseniz,
   eklentiler bu durumlarını kaybederler.
</p>

<table>
   <tr>
      <td colspan=2>
         Bu durumu silmek istediğinize emin misiniz?
      </td>
   </tr>
   <tr>
      <td>
         <a href="editattachstatuses.cgi?action=delete&id=[% id %]">
            Evet, sil
         </a>
      </td>
      <td align="right">
         <a href="editattachstatuses.cgi">
            Hayır, silme
         </a>
      </td>
   </tr>
</table>

[% PROCESS global/footer.html.tmpl %]

}

QProgressBar:chunk {
	background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(79, 104, 99, 180), stop:0.0247525 rgba(50, 65, 62, 180), stop:0.505658 rgba(48, 63, 63, 180), stop:0.536842 rgba(55, 72, 72, 180), stop:0.98594 rgba(71, 84, 85, 180), stop:1 rgba(121, 144, 145, 180));;

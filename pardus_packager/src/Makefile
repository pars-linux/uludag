#>+ 15
packager.cpp: $(srcdir)/packager.ui packager.h packager.moc
	rm -f packager.cpp
	echo '#include <kdialog.h>' > packager.cpp
	echo '#include <klocale.h>' >> packager.cpp
	$(UIC) -tr ${UIC_TR} -i packager.h $(srcdir)/packager.ui > packager.cpp.temp ; ret=$$?; \
	$(PERL) -pe "s,${UIC_TR}( \"\" ),QString::null,g" packager.cpp.temp | $(PERL) -pe "s,${UIC_TR}( \"\"\, \"\" ),QString::null,g" | $(PERL) -pe "s,image([0-9][0-9]*)_data,img\$$1_packager,g" >> packager.cpp ;\
	rm -f packager.cpp.temp ;\
	if test "$$ret" = 0; then echo '#include "packager.moc"' >> packager.cpp; else rm -f packager.cpp ; exit $$ret ; fi

packager.h: $(srcdir)/packager.ui
	$(UIC) -o packager.h $(srcdir)/packager.ui

packager.moc: packager.h
	$(MOC) packager.h -o packager.moc

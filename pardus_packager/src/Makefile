#>+ 21
clean-bcheck: 
	rm -f *.bchecktest.cc *.bchecktest.cc.class a.out

bcheck: bcheck-recursive

bcheck-am:
	@for i in ; do \
	    if test $(srcdir)/$$i -nt $$i.bchecktest.cc; then \
	        echo "int main() {return 0;}" > $$i.bchecktest.cc ; \
	        echo "#include \"$$i\"" >> $$i.bchecktest.cc ; \
	        echo "$$i"; \
	        if ! $(CXXCOMPILE)  --dump-class-hierarchy -c $$i.bchecktest.cc; then \
	            rm -f $$i.bchecktest.cc; exit 1; \
	        fi ; \
	        echo "" >> $$i.bchecktest.cc.class; \
	        perl $(top_srcdir)/admin/bcheck.pl $$i.bchecktest.cc.class || { rm -f $$i.bchecktest.cc; exit 1; }; \
	        rm -f a.out; \
	    fi ; \
	done


#>+ 11
pardus_packager.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/main.cpp $(srcdir)/packager_ui.cpp $(srcdir)/packager.cpp  packager_ui.moc packager.moc
	@echo 'creating pardus_packager.all_cpp.cpp ...'; \
	rm -f pardus_packager.all_cpp.files pardus_packager.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> pardus_packager.all_cpp.final; \
	for file in main.cpp packager_ui.cpp packager.cpp ; do \
	  echo "#include \"$$file\"" >> pardus_packager.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> pardus_packager.all_cpp.final; \
	done; \
	cat pardus_packager.all_cpp.final pardus_packager.all_cpp.files > pardus_packager.all_cpp.cpp; \
	rm -f pardus_packager.all_cpp.final pardus_packager.all_cpp.files


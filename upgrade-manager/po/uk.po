msgstr "Встановлено: <b>%s</b>"

#: src/gui.py:385
#, python-format
msgid "Upgraded: <b>%s</b>"
msgstr "Оновлено: <b>%s</b>"

#: src/gui.py:387
#, python-format
msgid "Configuring: <b>%s</b>"
msgstr "Налаштування: <b>%s</b>"

#: src/gui.py:389
#, python-format
msgid "Configured: <b>%s</b>"
msgstr "Налаштовано: <b>%s</b>"

#: src/gui.py:391
#, python-format
msgid "Removing: <b>%s</b>"
msgstr "Вилучення: <b>%s</b>"

#: src/gui.py:393
#, python-format
msgid "Removed: <b>%s</b>"
msgstr "Вилучено: <b>%s</b>"


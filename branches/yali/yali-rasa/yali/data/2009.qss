}

QTreeView {
        color: #642437;
        background-color: #eeeeee;
        border: 1px solid #6b6b6b;
        border-radius: 4px;
        selection-background-color: gray;
}

QTreeView::branch:has-siblings:!adjoins-item {
        border-image: url(:/images/treeview-vline.png) 0;
}

QTreeView::branch:has-siblings:adjoins-item {
        border-image: url(:/images/treeview-branch-more.png) 0;
}

QTreeView::branch:!has-children:!has-siblings:adjoins-item {
        border-image: url(:/images/treeview-branch-end.png) 0;
}

QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings {
         border-image: none;
         image: url(:/images/treeview-branch-closed.png);
}

QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings  {
         border-image: none;
         image: url(:/images/treeview-branch-open.png);
}

QHeaderView::section {
        background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                        stop: 0 #323232, stop: 0.4 #000000, stop: 1 #000000);
        color: #eeeeee;
        padding-left: 4px;
        border: 1px solid #6b6b6b;
}

QProgressBar {
        border: 1px solid #6b6b6b;
        border-radius: 4px;
        text-align: center;
        color: #642437;
        background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                        stop: 0 #ffffff, stop: 0.4 #eeeeee, stop: 1 #eeeeee);
}

QProgressBar:chunk {
        background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                        stop: 0 #693B59, stop: 1 #B7A2B0);
        border: 2px solid #642437;
        border-radius: 2px;
        margin: 2px;
}


QScrollArea::corner {
        background-color: rgba(255, 255, 255, 0);
}

QScrollBar:vertical {
        width: 16px;
        margin: 16px 0 16px 0;
        background-color: rgba(255, 255, 255, 0);
}

QScrollBar::handle:vertical {
        min-height: 16px;
        border: 2px solid #6b6b6b;
        background-color: #323232;
        border-radius: 3px;
}

QScrollBar::handle:vertical:hover {
        min-height: 16px;
        border: 2px solid #808080;
        background-color:  #323232;
        border-radius: 3px;
}

QScrollBar::add-line:vertical {
        border: none;
        height: 16px;
        subcontrol-position: bottom;
        subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
        border: none;
        height: 16px;
        subcontrol-position: top;
        subcontrol-origin: margin;
}

QScrollBar::up-arrow:vertical { background-image: url(:/images/arrow-up.png); }
QScrollBar::down-arrow:vertical { background-image: url(:/images/arrow-down.png); }

QScrollBar:horizontal {
        height: 16px;
        margin: 0 16px 0 16px;
        background-color: rgba(255, 255, 255, 0);
}

QScrollBar::handle:horizontal {
        min-width: 16px;
        border: 2px solid #6b6b6b;
        background-color:  #323232;
        border-radius: 3px;
}

QScrollBar::handle:horizontal:hover {
        min-width: 16px;
        border: 2px solid #808080;;
        background-color:  #323232;
        border-radius: 3px;
}

QScrollBar::add-line:horizontal {
        border: none;
        background: transparent;
        width: 16px;
        subcontrol-position: right;
        subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
        border: none;
        width: 16px;
        subcontrol-position: left;
        subcontrol-origin: margin;
}

QScrollBar::right-arrow:horizontal { background-image: url(:/images/arrow-right.png); }
QScrollBar::left-arrow:horizontal { background-image: url(:/images/arrow-left.png); }

QWidget{font:10pt;}

QComboBox {
        border: 1px solid #6b6b6b;
        border-radius: 4px;
        padding: 1px 18px 1px 3px;
        min-width: 6em;
        color: #642437;
}

QComboBox:editable {
        border-radius: 4px;
        border: 1px solid #6b6b6b;
        color: #642437;
        background: #eeeeee;
        selection-background-color: #0057ae;
}

QComboBox:!editable, QComboBox::drop-down:editable {
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                        stop: 0 #eeeeee, stop: 0.4 #eeeeee,
                                        stop: 0.5 #eeeeee, stop: 1.0 #eeeeee);
}

QComboBox:!editable:hover, QComboBox::drop-down:editable:hover {
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                        stop: 0 #eeeeee, stop: 0.4 #eeeeee,
                                        stop: 0.5 #eeeeee, stop: 1.0 #eeeeee);
        border: 1px solid #808080;
}

/* QComboBox gets the "on" state when the popup is open */
QComboBox:!editable:on, QComboBox::drop-down:editable:on {
          background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                        stop: 0 #eeeeee, stop: 0.4 #eeeeee,
                                        stop: 0.5 #eeeeee, stop: 1.0 #eeeeee);
}

QComboBox:on { /* shift the text when the popup opens */
       padding-top: 3px;
       padding-left: 4px;
}

QComboBox::drop-down {
       subcontrol-origin: padding;
       subcontrol-position: top right;
       width: 15px;
       border-left-width: 1px;
       border-left-color: #6b6b6b;
       border-left-style: solid; /* just a single line */
       border-top-right-radius: 4px; /* same radius as the QComboBox */
       border-bottom-right-radius: 3px;
}

QComboBox::drop-down:on {
       subcontrol-origin: padding;
       subcontrol-position: top right;
       width: 15px;
       border-left-width: 1px;
       border-left-color: #6b6b6b;
       border-left-style: solid; /* just a single line */
       border-top-right-radius: 4px; /* same radius as the QComboBox */
       border-bottom-right-radius: 3px;
}

QComboBox::down-arrow {
       image: url(:/images/arrow-down.png);
}

QComboBox::down-arrow:on { /* shift the arrow when popup is open */
       top: 1px;
       left: 1px;
}


/* The popup of the QComboBox */
QComboBox QAbstractItemView {
       color: #642437;
       selection-background-color: gray;
       background: #eeeeee;
}

QLineEdit {
       color: #642437;
       border: 1px solid #6b6b6b;
       border-radius: 4px;
       padding: 0 2px;
       background: #eeeeee;
       selection-background-color: gray;
}

QLineEdit:hover {
       color: #642437;
       border: 1px solid #808080;
       border-radius: 4px;
       padding: 0 2px;
       background: #eeeeee;
       selection-background-color: gray;
}

QLineEdit:disabled {
       color: #cccccc;
       border: 1px solid #444444;
       border-radius: 4px;
       padding: 0 2px;
       background: #000000;
       selection-background-color: gray;
}

QSpinBox {
        color: #642437;
        padding-right: 15px; /* make room for the arrows */
        border: 1px solid #6b6b6b;
        border-radius: 4px;
        background: #eeeeee;
        selection-background-color: gray;
}

QSpinBox:hover {
        color: #642437;
        padding-right: 15px; /* make room for the arrows */
        border: 1px solid #808080;
        border-radius: 4px;
        background: #eeeeee;
        selection-background-color: gray;
}

QSpinBox::up-button {
        subcontrol-origin: border;
        subcontrol-position: top right; /* position at the top right corner */
        width: 16px; /* 16 + 2*1px border-width = 15px padding + 3px parent border */
        border-image: url(:/images/arrow-up.png) 1;
        border-width: 1px;
}

QSpinBox::up-button:hover {
        background: #000000;
        background-color: #000000;
        border-radius: 4px;
        subcontrol-origin: border;
        subcontrol-position: top right; /* position at the top right corner */
        width: 16px; /* 16 + 2*1px border-width = 15px padding + 3px parent border */
        border-image: url(:/images/arrow-up.png) 1;
        border-width: 1px;
}

QSpinBox::down-button {
        subcontrol-origin: border;
        subcontrol-position: bottom right; /* position at bottom right corner */
        width: 16px;
        border-image: url(:/images/arrow-down.png) 1;
        border-width: 1px;
        border-top-width: 0;
}

QSpinBox::down-button:hover {
        background: #000000;
        border-radius: 4px;
        subcontrol-origin: border;
        subcontrol-position: bottom right; /* position at bottom right corner */
        width: 16px;
        border-image: url(:/images/arrow-down.png) 1;
        border-width: 1px;
        border-top-width: 0;
}

QTextEdit, QListView {
        color: #eeeeee;

/* #QTabWidget */

 QTabWidget::pane { /* The tab widget frame */
    /* border-top: 2px solid #C2C7CB;*/
border-top: 1px solid rgba(255,255,255,50);
 }

 QTabWidget::tab-bar {
     left: 0px; /* move to the right by 5px */
 }

 /* Style the tab using the tab sub-control. Note that
     it reads QTabBar _not_ QTabWidget */
 QTabBar::tab {
    background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1, stop:0 rgba(132, 132, 132, 255), stop:0.501481 rgba(70, 70, 70, 255), stop:0.503057 rgba(62, 62, 62, 255), stop:1 rgba(16, 16, 16, 255));
color:rgb(255,255,255);

border-top: 1px solid rgba(255,255,255,0);
border-bottom: 1px solid rgba(255,255,255,0);
border-left: 1px solid rgba(255,255,255,0);


     border-bottom: 0px; /* same as the pane color */
     border-top-left-radius: 2px;
     border-top-right-radius: 2px;
     min-width: 8ex;
     padding:4px;
 }

 QTabBar::tab:selected{
	background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(55, 55, 55, 255), stop:0.475 rgba(116, 116, 116, 255), stop:0.51 rgba(133, 133, 133, 255), stop:0.994975 rgba(166, 166, 166, 255));
 }

 QTabBar::tab:hover {
        	background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(55, 55, 55, 255), stop:0.475 rgba(116, 116, 116, 255), stop:0.51 rgba(133, 133, 133, 255), stop:0.994975 rgba(166, 166, 166, 255));
 }

 QTabBar::tab:selected {
     border-color:  rgba(255,255,255,0);
     border-bottom-color: #C2C7CB; /* same as pane color */
	background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(55, 55, 55, 255), stop:0.475 rgba(116, 116, 116, 255), stop:0.51 rgba(133, 133, 133, 255), stop:0.994975 rgba(166, 166, 166, 255));
 }

 QTabBar::tab:!selected {
     margin-top: 2px; /* make non-selected tabs look smaller */
 }




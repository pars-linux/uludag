            stop:0 rgba(171, 171, 171, 255), stop:1 rgba(255, 255, 255, 255));
    }

    QDateEdit:hover, QComboBox::drop-down:editable:hover { }

    QDateEdit::drop-down {
        subcontrol-origin: padding;
        subcontrol-position: top right;
        width: 22px;
        border-top-right-radius: 4px;
        border-bottom-right-radius: 3px;

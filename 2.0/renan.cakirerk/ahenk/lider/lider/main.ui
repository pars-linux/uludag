    QProgressBar {
        border-radius: 2px;
        text-align: center;
        color: #222222;
        background-color: white;
    }

    QProgressBar:chunk {
        background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0,
            stop:0 rgba(129, 3, 3, 255), stop:0.0192308 rgba(160, 35, 25, 255),
            stop:0.521739 rgba(166, 35, 29, 255), stop:0.531585 rgba(184, 46, 40, 255),
            stop:0.983696 rgba(168, 78, 74, 255), stop:1 rgba(210, 110, 110, 255));
        border: 1px solid rgba(255, 255, 255, 50);
        border-radius: 2px;
    }


/* #QScrollArea */

    QScrollArea::corner {
        background-color: rgba(255, 255, 255, 0);
        border: 0px;
    }

    QScrollBar:vertical {
        width: 13px;
        margin: 3px 3px 3px 3px;
        background-color: rgba(255, 255, 255, 0);
    }

    QScrollBar::handle:vertical {
        min-height: 16px;
        background-color: rgba(0,0,0, 150);
        border: 1px solid rgb(150, 150, 150);
        border-radius: 3px;
        padding:3px;
    }

    QScrollBar::handle:vertical:hover {
        min-height: 16px;
        background-color: rgba(50,50,50, 150);
        border-radius: 3px;
    }

    QScrollBar::add-line:vertical {
        border: 0px;
        height: 0px;
        subcontrol-position: bottom;
        subcontrol-origin: margin;
        background: transparent;
    }

    QScrollBar::sub-line:vertical {
        border: 0px;
        height: 0px;
        subcontrol-position: top;
        subcontrol-origin: margin;
        background: transparent;
    }

    QScrollBar:horizontal {
        height: 13px;
        margin: 3px 3px 3px 3px;
        background: transparent;
    }

    QScrollBar::handle:horizontal {
        min-width: 16px;
        background-color: rgba(0,0,0, 150);
        border: 1px solid rgb(150, 150, 150);
        border-radius: 3px;
    }

    QScrollBar::handle:horizontal:hover {
        min-width: 16px;
        background-color: rgba(50,50,50, 150);
        border-radius: 3px;
    }

    QScrollBar::add-line:horizontal {
        border: none;
        background: transparent;
        width: 0px;
        subcontrol-position: right;
        subcontrol-origin: margin;
    }

    QScrollBar::sub-line:horizontal {
        border: none;
        width: 0px;
        subcontrol-position: left;
        subcontrol-origin: margin;
    }
